<h1 style="text-align: center;">{{title}}</h1>
<div class="container" style="display:flex; gap:15px">
  {{#each dictionary}}

    <div class="card" style="width: 18rem;">
      <img
        src="https://i.pinimg.com/736x/d9/76/ae/d976ae0f673bbf313b9dc92cc1434e0d.jpg"
        class="card-img-top"
        alt="..."
      />
      <div class="card-body">
        <p class="card-test-bold">Term:</p>
        <p class="card-text">{{term}}</p>
        <p class="card-test-bold">Letter:</p>
        <p class="card-text">{{letter}}</p>
      </div>
    </div>
  {{/each}}
</div>

<div>
  <ul id="list-dicts">
  </ul>
</div>

<script src="./js/dictScript.js"></script>
<script>
  getDicts()
</script>